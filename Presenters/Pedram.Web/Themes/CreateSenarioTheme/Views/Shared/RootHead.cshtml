@using Pedram.Framework.StartUpClasses;
@using Pedram.Framework.SEO;
@using Pedram.Web.Models.Management;
<!-- Bootstrap core CSS -->


<meta name="viewport" content="width=device-width, initial-scale=1.0">

@{

    var _UserContext = SmObjectFactory.Container.GetInstance<Pedram.Framework.Helpers.IContextHelper>();
    var CurrentContext = _UserContext.GetCurrentContext();
    var RTL = CurrentContext.MyLanguage.RightToLeft;
   
    ViewBag.MetaDescription = @Seo.GenerateMetaTag(title: Seo.GeneratePageTitle(@ViewBag.Title),
                       description: @ViewBag.Description,
                       allowIndexPage: true,
                       allowFollowLinks: true,
                       cacheControlType: Seo.CacheControlType._public,
                       language: CurrentContext.MyLanguage.LangName,
                       copywrite: Configurations.copywrite

                   );

}
@Html.Raw(ViewBag.MetaDescription)

